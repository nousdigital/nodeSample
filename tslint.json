{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": false,
    "no-console": false,
    "interface-name" : [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "quotes": [2, "single", { "avoidEscape": true }]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  }
}
